{
  "name": "zezzman.github.io",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/material": "^5.10.11",
    "@react-page/editor": "^5.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "typed-scss-modules src/**/*.module.scss & react-scripts start",
    "build": "typed-scss-modules src/**/*.module.scss & react-scripts build",
    "test": "typed-scss-modules src/**/*.module.scss & react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^16.18.2",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "gh-pages": "^4.0.0",
    "node-sass": "^7.0.3",
    "react-scripts": "5.0.1",
    "source-map-explorer": "^2.5.3",
    "typed-scss-modules": "^7.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
